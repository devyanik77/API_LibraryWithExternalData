			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"let rawBody = pm.response.text();\r",
							"let statusCode = pm.response.code;\r",
							"let bookId = pm.collectionVariables.get(\"book_id\");\r",
							"\r",
							"let jsonData = {};\r",
							"let hasJson = false;\r",
							"\r",
							"if (rawBody && rawBody.trim().length > 0) {\r",
							"    try {\r",
							"        jsonData = pm.response.json();\r",
							"        hasJson = true;\r",
							"    } catch (e) {\r",
							"        console.log(\"PATCH Response is not valid JSON:\", rawBody);\r",
							"    }\r",
							"} else {\r",
							"    console.log(\"PATCH Response body is empty.\");\r",
							"}\r",
							"\r",
							"pm.test(\"PATCH - Status code is 200\", function () {\r",
							"    pm.expect(statusCode).to.eql(200);\r",
							"});\r",
							"\r",
							"pm.test(\"PATCH - Content-Type header present\", function () {\r",
							"    pm.response.to.have.header(\"Content-Type\");\r",
							"});\r",
							"\r",
							"pm.test(\"PATCH - Validate required fields exist\", function () {\r",
							"    if (hasJson) {\r",
							"        pm.expect(jsonData).to.have.property(\"id\");\r",
							"        pm.expect(jsonData).to.have.property(\"title\");\r",
							"        pm.expect(jsonData).to.have.property(\"author\");\r",
							"        pm.expect(jsonData).to.have.property(\"genre\");\r",
							"        pm.expect(jsonData).to.have.property(\"yearPublished\");\r",
							"        pm.expect(jsonData).to.have.property(\"checkedOut\");\r",
							"    } else {\r",
							"        console.log(\"Skipping field validation (no JSON)\");\r",
							"    }\r",
							"});\r",
							"\r",
							"pm.test(\"PATCH - ID remains unchanged\", function () {\r",
							"    if (hasJson) {\r",
							"        pm.expect(jsonData.id).to.eql(bookId);\r",
							"    }\r",
							"});\r",
							"\r",
							"pm.test(\"PATCH - Validate checkedOut updated to true\", function () {\r",
							"    if (hasJson) {\r",
							"        pm.expect(jsonData.checkedOut).to.eql(true);\r",
							"    }\r",
							"});\r",
							"\r",
							"pm.test(\"PATCH - Body contains updated fields\", function () {\r",
							"    if (hasJson) {\r",
							"        pm.expect(rawBody.toLowerCase()).to.include(\"author\");\r",
							"    }\r",
							"});\r",
							"\r",
							"pm.test(\"PATCH - Response time < 1000ms\", function () {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(500);\r",
							"});\r",
							"\r",
							"pm.test(\"PATCH - Response JSON is valid\", function () {\r",
							"    pm.expect(() => JSON.parse(rawBody)).to.not.throw();\r",
							"});\r",
							"\r",
							"pm.test(\"PATCH - No empty fields\", function () {\r",
							"    if (hasJson) {\r",
