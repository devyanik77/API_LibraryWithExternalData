		{
			"name": "DeleteBookById",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"let rawBody = pm.response.text();\r",
							"let statusCode = pm.response.code;\r",
							"let bookId = pm.collectionVariables.get(\"book_id\");\r",
							"\r",
							"let jsonData = {};\r",
							"let hasJson = false;\r",
							"\r",
							"if (rawBody && rawBody.trim().length > 0) {\r",
							"    try {\r",
							"        jsonData = pm.response.json();\r",
							"        hasJson = true;\r",
							"    } catch (e) {\r",
							"        console.log(\"Response is not valid JSON:\", rawBody);\r",
							"    }\r",
							"} else {\r",
							"    console.log(\"Response body is empty.\");\r",
							"}\r",
							"\r",
							"pm.test(\"DELETE - Status code is correct\", function () {\r",
							"    pm.expect(statusCode).to.be.oneOf([200, 204, 404]);\r",
							"});\r",
							"\r",
							"pm.test(\"DELETE - Content-Type header (if body exists)\", function () {\r",
							"    if (rawBody.trim().length > 0) {\r",
							"        pm.response.to.have.header(\"Content-Type\");\r",
							"    }\r",
							"});\r",
							"\r",
							"pm.test(\"DELETE - Validate status and messages\", function () {\r",
							"\r",
							"    if (statusCode === 200 || statusCode === 204) {\r",
							"        console.log(\"Book deleted successfully.\");\r",
							"        pm.expect([200, 204]).to.include(statusCode);\r",
							"    }\r",
							"\r",
							"    else if (statusCode === 404 && hasJson && jsonData.message) {\r",
							"        const expectedMsg = `Book with id '${bookId}' not found`;\r",
							"        console.log(\"DELETE returned 404:\", jsonData.message);\r",
							"\r",
							"        pm.expect(jsonData.message.toLowerCase()).to.include(\"not found\");\r",
							"        pm.expect(jsonData.message).to.include(bookId);\r",
							"    } \r",
							"    \r",
							"    else {\r",
							"        console.log(\"DELETE returned unexpected body:\", rawBody);\r",
							"    }\r",
							"});\r",
							"\r",
							"pm.test(\"DELETE - If message exists, confirm it mentions deleted book\", function () {\r",
							"    if (hasJson && jsonData.message) {\r",
							"        pm.expect(jsonData.message).to.include(\"Book with id\");\r",
							"        pm.expect(jsonData.message).to.include(bookId);\r",
							"    } else {\r",
							"        console.log(\"No 'message' field to validate.\");\r",
							"    }\r",
							"});\r",
							"\r",
							"pm.test(\"GET after DELETE - Correct not found message\", function () {\r",
							"    if (statusCode === 404 && hasJson && jsonData.message) {\r",
							"\r",
							"        console.log(\"Validating GET-after-DELETE message...\");\r",
							"\r",
							"        pm.expect(jsonData.message.toLowerCase()).to.include(\"not found\");\r",
							"        pm.expect(jsonData.message).to.include(bookId);\r",
							"\r",
							"    } else {\r",
							"        console.log(\"Skipping message validation (no JSON or not 404).\");\r",
							"    }\r",
							"});\r",
							"\r",
							"pm.test(\"GET after DELETE - Response time < 1000ms\", function () {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(1000);\r",
							"});"
						],
