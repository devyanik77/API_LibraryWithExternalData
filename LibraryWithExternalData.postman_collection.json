				{
					"listen": "test",
					"script": {
						"exec": [
							"let jsonData = pm.response.json();\r",
							"console.log(\"Stored book_id:\", jsonData.id);\r",
							"\r",
							"pm.test(\"Validate Status code is 201\", function () {\r",
							"    pm.response.to.have.status(201);\r",
							"});\r",
							"\r",
							"pm.collectionVariables.set(\"book_id\", jsonData.id);\r",
							"\r",
							"pm.test(\"Validate Book ID saved\", function() {\r",
							"    pm.expect(pm.collectionVariables.get(\"book_id\")).to.eql(jsonData.id);\r",
							"});\r",
							"\r",
							"pm.test(\"Validate required fields exist\", function() {\r",
							"    pm.expect(jsonData).to.have.property(\"id\");\r",
							"    pm.expect(jsonData).to.have.property(\"title\");\r",
							"    pm.expect(jsonData).to.have.property(\"author\");\r",
							"    pm.expect(jsonData).to.have.property(\"genre\");\r",
							"    pm.expect(jsonData).to.have.property(\"yearPublished\");\r",
							"});\r",
							"\r",
							"pm.test(\"Validate important fields are not null\", function() {\r",
							"    pm.expect(jsonData.id).to.not.be.null;\r",
							"    pm.expect(jsonData.checkedOut).to.not.be.undefined;\r",
							"});\r",
							"\r",
							"pm.test(\"Validate checkedOut is false\", function() {\r",
							"    pm.expect(jsonData.checkedOut).to.eql(false);\r",
							"});\r",
							"\r",
							"pm.test(\"Validate yearPublished is a number\", function() {\r",
							"    pm.expect(jsonData.yearPublished).to.be.a(\"number\");\r",
							"});\r",
							"\r",
							"pm.test(\"Validate Content-Type header\", function () {\r",
							"    pm.response.to.have.header(\"Content-Type\");\r",
							"});\r",
							"\r",
							"pm.test(\"Validate response time < 1000ms\", function () {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(1000);\r",
							"});\r",
							"\r",
							"pm.test(\"Response is valid JSON\", function () {\r",
							"    pm.expect(() => JSON.parse(pm.response.text())).to.not.throw();\r",
